{
  "name": "lemric/waf",
  "description": "Web Application Firewall for Symfony",
  "keywords": ["waf", "symfony"],
  "support": {
    "issues": "https://github.com/Lemric/waf/issues",
    "security": "https://github.com/Lemric/waf/security"
  },
  "authors": [
    {
      "name": "Dominik Labudzinski",
      "email": "dominik@labudzinski.com",
      "homepage": "https://labudzinski.com"
    },
    {
      "name": "Lemric",
      "homepage": "https://lemric.com"
    }
  ],
  "license": "GPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "Lemric\\WAF\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lemric\\WAF\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=8.3",
    "symfony/framework-bundle": "^6.4 || ^7.1",
    "symfony/security-core": "^6.4 || ^7.1",
    "symfony/yaml": "^6.4 || ^7.1",
    "foroco/php-browser-detection": "^v2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3.1"
  }
}